/*视频*/
/*通用部分*/
.video_1_box,.video_2_box,.video_3_box{
	width:100%;
	height:100%;
	position:relative;
}
.video_1,.video_2,.video_3{
	width: 100%;
    height: 100%;
	margin-top:1%;
}
/*第一个视频单独部分*/
.video_1_logobox{
	width:50%;
	height:50%;
	position:absolute;
	left:0;right:0;top:40%;bottom:0;
	margin:0 auto;
}
.video_1_logo{
	width:100%;
	height:100%;
}
/*第二、三、四个视频部分*/
.video_2_fontbox,.video_3_fontbox{
	width:30%;
	height:30%;
	position:absolute;
	left:0;right:0;top:40%;bottom:0;
	margin:0 auto;
	cursor:default;
}
.video_2_font1,.video_3_font1{
	display: block;
    font-size:5vw;
	font-weight:900;
	font-style: italic;
	color:white;
}
.video_2_font2,.video_3_font2{
	display: block;
    font-size:5vw;
	font-weight:900;
	font-style: italic;
	color:rgb(230,152,0);
}
.video_2_font3,.video_3_font3{
	display: block;
	font-size:1vw;
	font-style: italic;
	color:white;
}
/*天气地形部分*/
.weather_box{
	display:flex;
	width:100%;
	height:100%;
	position:absolute;
	top:0;
	left:0;
	overflow:hidden;
	z-index:100;
}
.weather_dx_box{
	position:relative;
	width:100%;
	height:100%;
	cursor:default;
}
/*天气图片部分*/
.weather_1,.weather_2,.weather_3{
	position:relative;
	height:100%;
	overflow:hidden;
	background:#000;
	transition:all 0.1s;
}
.weather_1_img,.weather_3_img{
	height:100%;
	transition:all 2s;
}
.weather_2_img{
	height:100%;
	transition:all 0.5s;
}
/*天气图片上的文字部分*/
.weather_dx_font_box{
	width:100%;
	height:100%;
	position:absolute;
	left:0;
	top:0;
}
.weather_1_font,.weather_2_font,.weather_3_font{
	width:100px;
	height:50px;
	font-size:3vw;
	color:white;
	position:absolute;
	left:0;right:0;top:20%;bottom:0;
	margin:auto auto;
}
/*地形图片以及图片上的文字*/
.dx_box{
	display:flex;
	width:100%;
	height:100%;
	position:relative;
	overflow:hidden;
}
.dx_1,.dx_2,.dx_3,.dx_4,.dx_5{
	position:relative;
	height:100%;
	overflow:hidden;
	background:#000;
	transition:all 0.1s;
}
.dx_1_img,.dx_2_img,.dx_4_img,.dx_5_img{
	height:100%;
	transition:all 2s;
}
.dx_3_img{
	height:100%;
	transition:all 0.5s;
}
.dx_1_font,.dx_2_font,.dx_3_font,.dx_4_font,.dx_5_font{
	width:100px;
	height:50px;
	font-size:3vw;
	color:white;
	position:absolute;
	left:0;right:0;top:20%;bottom:0;
	margin:auto auto;
}
/*天气地形文字部分*/
.weather_font{
	position:absolute;
	left:25%;
	top:70%;
	font-size:3vw;
	font-weight:900;
	color:white;
	text-decoration:underline;
	z-index:102;
}
.weather_font2{
	position:absolute;
	left:15%;
	top:80%;
	font-size:1vw;
	color:white;
	z-index:102;
}
.dx_font{
	position:absolute;
	right:22%;
	top:70%;
	font-size:3vw;
	font-weight:900;
	color:white;
	text-decoration:underline;
	z-index:102;
}
.dx_font2{
	position:absolute;
	right:15%;
	top:80%;
	font-size:1vw;
	color:white;
	z-index:102;
}
/*地图部分*/
.map_box{
	position:relative;
	width:100%;
	height:100%;
	background:url("../img/bg_section5.jpg") no-repeat;
	background-position:center center;
	background-size:cover;
}
.map_font_1{
	position:absolute;
	left:0;right:0;top:0;bottom:0;
	margin:8% auto;
	color:rgb(230,152,0);
	font-size:4vw;
	width:40%;
	text-align:center;
	font-style: italic;
	cursor:default;
}
.map_font_2{
	position:absolute;
	left:0;right:0;top:0;bottom:0;
	margin:13% auto;
	color:white;
	font-size:1vw;
	width:40%;
	text-align:center;
	cursor:default;
}
/*地图中的小圆圈部分*/
.quan_jia{
	position:absolute;
	left:0;top:0;bottom:0;right:0;
	margin:auto auto;
	color:rgb(230,152,0);
	display:inline-block;
	width:20px;
	height:20px;
	border-radius:50%;
	background-color:white;
	text-align:center;
	line-height:18px;
	z-index:111;
}
.quan_da{
	position:absolute;
	left:0;top:0;bottom:0;right:0;
	margin:auto auto;
	background-color:white;
	border-radius:50%;
	transition:all 1s;
}
.quan_zhong,.quan_xiao{
	position:absolute;
	left:0;top:0;bottom:0;right:0;
	margin:auto auto;
	border-radius:50%;
	background-color:white;
	transition:all 1s;
}
.quan_box{
	position:absolute;
	left:38%;
	top:40%;
	z-index:110;
	width:60px;
	height:60px;
	border-radius:50%;
	cursor:pointer;
}
.quan_box_2{
	position:absolute;
	left:60%;
	top:58%;
	z-index:110;
	width:60px;
	height:60px;
	border-radius:50%;
	cursor:pointer;
}
/*地图上的视频*/
.map_video_box{
	width:20%;
	height:34%;
	border-radius:50%;
	position:absolute;
	left:38%;
	top:40%;
	overflow:hidden;
	display:none;
}
.map_video{
	width:200%;
	transform:translate(-25%);
}
/*点击切屏*/
#menu{
	position:fixed;
	left:6%;
	top:-4%;
	font-size:1.5vw;
}
#menu>li{padding-top:130px;}
#menu a{color:white;}
/*右侧的滚动条*/
.index_nav_box{
	height:85%;
	width:2.2%;
	position:fixed;
	left:2%;
	top:10%;
	z-index:150;
}
.index_nav{
	width:100%;
	height:100%;
}